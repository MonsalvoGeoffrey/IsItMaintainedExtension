{
    "name": "Is It Maintained?",
    "description": "See at a glance whehter a github repository is still resolving issues or not.",
    "version": "1.0",
    "manifest_version": 3,
    "host_permissions": [
        "*://*.github.com/*",
        "*://isitmaintained.com/*"
    ],
    "content_scripts": [
    {
        "matches": ["*://*.github.com/*"],
        "js": ["content-script.js"]
    }
    ],
    "icons": {
        "128": "imgs/icon.png"
    }
  }